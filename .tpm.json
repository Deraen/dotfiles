{
  "verbose": true,
  "packages": {
    "tpm": "baabelfish/tpm",
    "dspmgr": "Deraen/dspmgr",
    "pwrmgr": "Deraen/pwrmgr",
    "tpm-filemanagement": "baabelfish/tpm-filemanagement",
    "liquidprompt": {
      "url": "nojhan/liquidprompt",
      "source": ["liquidprompt"]
    },
    "urxvt-perls": "muennich/urxvt-perls",
    "urxvt-font-size": "majutsushi/urxvt-font-size",
    "xcape": {
      "url": "alols/xcape",
      "bin": {
        "xcape": "xcape"
      },
      "build": "make"
    },
    "dunst": {
      "url": "knopwob/dunst",
      "bin": {
        "dunst": "dunst"
      },
      "build": "make"
    },
    "ponymix": {
      "url": "falconindy/ponymix",
      "bin": {
        "ponymix": "ponymix"
      },
      "build": "make"
    },
    "i3situation": {
      "url": "HarveyHunt/i3situation",
      "build": "python3 setup.py install --user"
    }
  }
}
