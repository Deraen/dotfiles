Xft.autohint: 1
Xft.hintstyle:  hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
Xft.dpi: 96

URxvt*background: #121212
URxvt*foreground: #d0d0d0
URxvt*color0: #4e4e4e
URxvt*color1: #d68787
URxvt*color2: #5f865f
URxvt*color3: #d8af5f
URxvt*color4: #85add4
URxvt*color5: #d7afaf
URxvt*color6: #87afaf
URxvt*color7: #d0d0d0
URxvt*color8: #626262
URxvt*color9: #d75f87
URxvt*color10: #87af87
URxvt*color11: #ffd787
URxvt*color12: #add4fb
URxvt*color13: #ffafaf
URxvt*color14: #87d7d7
URxvt*color15: #e4e4e4
URxvt*cursorColor: #d0d0d0
URxvt*cursorColor2: #3a3a3a
URxvt*colorBD: #e4e4e4

URxvt*imLocale:          en_US.utf8
URxvt.bigfont_key:       o
URxvt.borderLess:        no
URxvt.buffered:          false
URxvt.depth:             32
URxvt.letterSpace:       -2

URxvt*font:              xft:Liberation Mono:pixelsize=13

URxvt.hold:              false
URxvt.urgentOnBell:  true
URxvt.internalBorder:    7
URxvt.jumpScroll:        true
URxvt.loginShell:        true
URxvt.secondaryScreen:   false
URxvt.skipScroll:        true
URxvt.smallfont_key:     p
URxvt.tripleclickwords:  on
URxvt*keysym.Control-Up:    \033[1;5A
URxvt*keysym.Control-Down:  \033[1;5B
URxvt*keysym.Control-Left:  \033[1;5D
URxvt*keysym.Control-Right: \033[1;5C

urxvt*saveLines:            50000
URxvt.scrollBar:            off
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true

URxvt.perl-lib: /home/juho/.local/modules/urxvt-font-size:/home/juho/.config/terminal/urxvt/perl
URxvt.perl-ext: default,font-size,url-select,selection-to-clipboard
URxvt.keysym.M-u:      perl:url-select:select_next
URxvt.keysym.C-Up:     perl:font-size:increase
URxvt.keysym.C-Down:   perl:font-size:decrease
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.url-launcher:       open-url.sh

rofi.case-sensitive: false
rofi.fg: #fff
rofi.font: Ubuntu 17
rofi.color-normal:                   #3C3B37,#fff,#3C3B37,#586e75,#eee8d5
rofi.color-urgent:                   #3C3B37,#dc322f,#eee8d5,#dc322f,#fdf6e3
rofi.color-active:                   #3C3B37,#268bd2,#eee8d5,#268bd2,#fdf6e3
rofi.color-window:                   #3C3B37,#3C3B37
rofi.location: 6
rofi.display-drun:
rofi.modi: drun,run,window
